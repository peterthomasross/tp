{
  "name": "Target Process Github",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "Target process optimisations for github",
  "homepage_url": "http://river.red",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "default_locale": "en",
  "content_scripts": [
    {
      "matches": [
        "https://github.com/RedRiverSoftware/*"
      ],
      "js": [
        "src/inject/inject.js"
      ]
    }
  ]
}
